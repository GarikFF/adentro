<%
const config = {
	xMargin: 50,
	schemaWidth: 800,
	height: 60,
	blockHeight: 20,
	barWidth: 3
};
%>

<svg width="<%= config.schemaWidth + (config.xMargin * 2) %>" height="<%= config.height * scheme.length %>" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" overflow="hidden">
	<% scheme.forEach((elements, line) => { %>
		<%- include('part', {config, elements, line: line + 1}); %>
	<% }); %>
</svg>
