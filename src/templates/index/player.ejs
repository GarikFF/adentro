<div id="jplayer" class="jp-jplayer"></div>
<div id="jp_container_1" class="jp-audio">
	<div class="jp-type-single">
		<div class="jp-gui jp-interface">
			<a href="javascript:;" class="jp-control jp-play" tabindex="1">play</a>
			<a href="javascript:;" class="jp-control jp-pause" tabindex="1">pause</a>
			<a href="javascript:;" class="jp-control jp-stop" tabindex="1">stop</a>
			<div class="player__progress-wrapper">
				<div class="jp-progress">
					<div class="jp-seek-bar">
						<div class="jp-play-bar"></div>
						<div class="jp-details">
							<ul>
								<li><span class="jp-title"></span></li>
							</ul>
						</div>
					</div>
				</div>
				<div class="jp-time-holder">
					<div class="jp-current-time"></div>
					<div class="jp-duration"></div>
				</div>
			</div>
			<a href="javascript:;" class="jp-control jp-mute" tabindex="1" title="mute">mute</a>
			<a href="javascript:;" class="jp-control jp-unmute" tabindex="1" title="unmute">unmute</a>
			<div class="jp-volume-bar">
				<div class="jp-volume-bar-value"></div>
			</div>
			<a href="javascript:;" class="jp-control jp-volume-max" tabindex="1" title="max volume">max volume</a>
		</div>
		<div class="jp-no-solution">
			<span>Update Required</span>
			To play the media you will need to either update your browser to a recent version or update your
			<a href="http://get.adobe.com/flashplayer/" target="_blank">Flash plugin</a>.
		</div>
	</div>
</div>
